<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:style src="MainApplicationStyle2.css" />
	
	<g:DockLayoutPanel unit='PCT'>
		<g:north size='10'>
			<g:FlowPanel ui:field="headerPanel"
				styleName="{style.padding} {style.northPanel}">
				<g:Label>This is the NORTH panel</g:Label>
			</g:FlowPanel>
		</g:north>
		<g:center>
			<g:SplitLayoutPanel>
				<g:west size='500'>

					<g:SimpleLayoutPanel ui:field="westPanel"
						styleName="{style.padding}">
					</g:SimpleLayoutPanel>

				</g:west>

				<g:center>
<!-- 					<g:SimpleLayoutPanel ui:field="centerPanel" styleName=" {style.padding}"> -->
						<g:ScrollPanel ui:field="centerPanel" styleName=" {style.padding}">
						
						</g:ScrollPanel>
						<!-- <g:FlowPanel ui:field="centerPanel"> -->
						<!-- <g:Label styleName="{style.YellowText}" >This is the CENTER panel</g:Label> -->
<!-- 					</g:SimpleLayoutPanel> -->
				</g:center>

				<g:south size="150">
					<g:FlowPanel styleName="{style.padding}">
						<g:Label>This is the SOUTH panel</g:Label>
					</g:FlowPanel>
				</g:south>

			</g:SplitLayoutPanel>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder> 
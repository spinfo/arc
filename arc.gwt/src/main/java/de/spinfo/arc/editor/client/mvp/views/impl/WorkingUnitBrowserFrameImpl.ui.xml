<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style src="MainApplicationStyle2.css">
	.title {
		text-align: center;
		vertical-align: middle;
		padding: 5px;
		background-color: silver;
		font-size: 20px;
		font-weight: 900;
	}
	
	.infoBlock {
		
	}
	
	.info {
		text-align: center;
		margin-left: auto;
		margin-right: auto;
		padding: 10px;
		background-color: #E0F5FF;
		font-size: 16px;
		font-weight: normal;
	}
	
	.subInfo {
		text-align: left;
		margin-left: auto;
		margin-right: auto;
		padding: 15px;
		background-color: #EBF5FF;
		font-size: 20px;
		font-weight: normal;
	}
	
	.bg {
		background-color: #E0F5FF;
	}
	</ui:style>
	<g:SplitLayoutPanel ui:field="splitPanel">
		<g:west size='500'>
			<g:DockLayoutPanel unit="PCT">
				<g:north size="1">
					<g:SimpleLayoutPanel></g:SimpleLayoutPanel>
				</g:north>
				<g:center>
					<g:DockLayoutPanel unit="PCT">
						<g:north size="8">
							<g:HorizontalPanel  ui:field="NavigationTitlePanel" width="100%" height="100%">
								<g:Label addStyleNames="{style.title}">Working Units Browser</g:Label>
							</g:HorizontalPanel>
						</g:north>
						<g:center>
							<g:SimpleLayoutPanel ui:field="westPanel"
								styleName="{style.padding}">

								<!-- <g:Label styleName="{style.YellowText}" >This is the CENTER 
									panel</g:Label> -->
							</g:SimpleLayoutPanel>
						</g:center>
					</g:DockLayoutPanel>
				</g:center>
				<g:east size="1">
					<g:SimpleLayoutPanel></g:SimpleLayoutPanel>
				</g:east>
				<g:south size="1">
					<g:SimpleLayoutPanel></g:SimpleLayoutPanel>
				</g:south>
				<g:west size="1">
					<g:SimpleLayoutPanel></g:SimpleLayoutPanel>
				</g:west>
			</g:DockLayoutPanel>
		</g:west>

		<g:center>
			<g:SimpleLayoutPanel ui:field="centerPanel"
				styleName=" {style.padding}">

			</g:SimpleLayoutPanel>
		</g:center>
	</g:SplitLayoutPanel>
</ui:UiBinder> 
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.title {
		font-size: 18px;
	}
	</ui:style>
	<g:HTMLPanel>
		<table width="100%" height="100%">
			<tr>
				<td>
					<g:HorizontalPanel>
						<g:Image url="gwtcomp-icons/edit_add.png" />
						<g:Label ui:field="titleLabel" styleName="{style.title}">Add new Range</g:Label>

					</g:HorizontalPanel>
				</td>

			</tr>
			<tr>
				<td>
					<g:Label ui:field="questionToUserLabel" styleName="{style.title}">enter
						title</g:Label>
				</td>
				<!-- <td><g:Label ui:field="categoryOrdinal">enter ordinal</g:Label></td> -->
			</tr>
			<tr>
				<td>
					<g:TextBox ui:field="rangeTitleTextBox" width="90%"
						height="100%" />
					<g:ListBox ui:field="rangeTitleListBox" width="90%"
						height="100%">
						<g:item>Sursilvan</g:item>
						<g:item>Sutsilvan</g:item>
						<g:item>Surmiran</g:item>
						<g:item>Puter</g:item>
						<g:item>Vallader</g:item>
						<g:item>Sutsettisch</g:item>
						<g:item>Bivio</g:item>
						<g:item>Bergagliot</g:item>
						<g:item>Val Müstair</g:item>
						<g:item>Bühlers Koine</g:item>
						<g:item>Jauer</g:item>
						<g:item>Deutsch/Tudesg</g:item>
						<g:item>Latein</g:item>
						<g:item>Italiano</g:item>
						<g:item>Auters</g:item>
					</g:ListBox>
				</td>
			</tr>
			<tr>
				<td>
					<g:Label ui:field="newStartOfRangeLabel">Select this word as start of range type</g:Label>
					<g:Label ui:field="typeOfRangeLabel">Chapter_Range</g:Label>
					<g:HTMLPanel ui:field="upperLine">
						<hr style="width:100%;" />
					</g:HTMLPanel>
				</td>
			</tr>
			<tr>
				<td>
					<g:Label ui:field="selectedWordLabel" styleName="{style.title}"></g:Label>
				</td>
			</tr>
			<tr align="left">
				<td>
					<hr style="width:100%;" />
					<g:HorizontalPanel ui:field="buttonContainer">
						<!-- <g:Button ui:field="saveRangeBtn">save range</g:Button> -->
						<!-- <g:Button ui:field="cancelBtn">cancel and close</g:Button> -->
					</g:HorizontalPanel>
				</td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 
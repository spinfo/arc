<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
<!-- 		font-weight: bold; -->
		padding: 3px;
		cursor: pointer;
	}
	</ui:style>
	<g:FocusPanel ui:field="panel">
	<g:HorizontalPanel>
		<g:Label styleName="{style.important}" ui:field="questionToUser" />
		<g:Label styleName="{style.important}" ui:field="type" />
	</g:HorizontalPanel>
	</g:FocusPanel>
</ui:UiBinder> 